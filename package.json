{
  "name": "greeting-tool",
  "displayName": "Greeting Tool",
  "description": "A VS Code extension that provides template processing chat participants for VS Code Copilot.",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/workspace-analyzer/vscode-workspace-analyzer.git"
  },
  "engines": {
    "vscode": "^1.85.0",
    "node": ">=20.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "chatParticipants": [
      {
        "id": "workspace-analyzer.template",
        "name": "template",
        "fullName": "Template Processor",
        "description": "Debug chat context and display context information",
        "isSticky": false,
        "commands": [
          {
            "name": "process",
            "description": "Debug and display the complete chat context"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package",
    "start": "NODE_NO_WARNINGS=1 node --no-deprecation ./out/extension.js"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
